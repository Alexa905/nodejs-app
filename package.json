{
  "name": "nodejs-training",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "nodemon index.js --exec babel-node",
    "start:postgres": "npm run db:create && npm run db:migrate && npm run db:fill",
    "db:create": "node_modules/.bin/sequelize db:create",
    "db:drop": "node_modules/.bin/sequelize db:drop",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo": "node_modules/.bin/sequelize db:migrate:undo",
    "db:fill": "babel-node ./modules/migrator.js",
    "start:mongo": "babel-node ./mongodb/index.js",
    "mongodb:migrate": "babel-node ./mongodb/migrator.js"
  },
  "author": "Tatyana Alexeenkova",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "minimist": "^1.2.0",
    "nodemon": "^1.17.5",
    "request": "^2.87.0",
    "split": "^1.0.1",
    "through2": "^2.0.3"
  },
  "dependencies": {
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^4.13.13",
    "papaparse": "^4.4.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^6.4.2",
    "sequelize": "^4.37.8",
    "sequelize-cli": "^3.2.0"
  }
}
